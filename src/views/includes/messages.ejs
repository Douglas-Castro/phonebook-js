<% if (errors.length > 0) { %>
<div class="row">
  <div class="col my-2">
    <div class="alert alert-danger">
      <% errors.forEach(error => { %>
        <%= error %><br>
      <% }) %>
    </div>
  </div>
</div>
<% } %>

<% if (success.length > 0) { %>
<div class="row">
  <div class="col my-1">
    <div class="alert alert-warning">
      <% success.forEach(success => { %>
        <%= success %><br>
      <% }) %>
    </div>
  </div>
</div>
<% } %>
